<ul class="ig_posts">
{% for post in posts %}
  <li class="post">
    <a href="{{ post.get('permalink') }}" target="_blank">
      <div class="caption">
        {{ post.get('caption') }}
      </div>

      <div class="image">
        <img class="responsive-image lazyload" src="{{ post.get('media_url') }}" alt="{{ post.get('caption')}} " />
      </div>
    </a>
  </li>
{% endfor %}
</ul>
